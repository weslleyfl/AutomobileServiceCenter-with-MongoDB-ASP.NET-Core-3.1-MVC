@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Reset password confirmation";
}


<div class="row">
    <div class="col s12">
        <div class="card-panel orange lighten-1 margin-bottom-50px margin-top-50px">
            <span class="white-text">
                Sua senha foi alterada. Por favor
                <a asp-area="Identity" asp-page="/Account/Login">Clique aqui para Logar</a>.
            </span>
        </div>
    </div>
</div>